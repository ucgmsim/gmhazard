{
  "name": "seistech-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "PORT=${PORT} react-scripts start -- --node-flags --max-old-space-size=1024",
    "start:dev": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` REACT_APP_BUILD_DATE=$(date +%Y-%m-%d)-$(date +%T) PORT=5100 env-cmd -f .env.dev react-scripts start --max-old-space-size=1024",
    "start:ea": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` REACT_APP_BUILD_DATE=$(date +%Y-%m-%d)-$(date +%T) PORT=5100 env-cmd -f .env.test react-scripts start --max-old-space-size=1024",
    "build": "env-cmd -f .env.production react-scripts build -- --node-flags --max-old-space-size=1024",
    "build:dev": "env-cmd -f .env.development npm run build ",
    "build:test": "env-cmd -f .env.test npm run build ",
    "build:staging": "env-cmd -f .env.staging npm run build ",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy-dev": "node deploy-ftp-azure seistech-dev",
    "deploy-test": "node deploy-ftp-azure seistech-test",
    "deploy-prod": "node deploy-ftp-azure seistech-prod"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.12.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.11.2",
    "auth0": "^2.28.0",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.2",
    "dompurify": "^2.2.6",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "jquery": "^3.5.1",
    "plotly.js": "^1.55.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-center": "^1.1.1",
    "react-dom": "^16.13.1",
    "react-error-overlay": "^6.0.9",
    "react-map-gl": "^5.2.8",
    "react-markdown": "^5.0.3",
    "react-plotly.js": "^2.5.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-select": "^3.1.0",
    "react-spinners": "^0.8.3",
    "reactstrap": "^8.5.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {},
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
